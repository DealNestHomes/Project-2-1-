{
  "version": 2,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile --ignore-scripts",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}

